# Disclaimer
echo -e "$divider"
echo -e $R'DISCLAIMER'$N
echo -e "$divider"
echo -e $R"THIS IS SIMPLY A MODULE TO CHANGE YOUR SYSTEMLESS"$N
echo -e $R"HOSTS FILE. IF YOU DON'T KNOW WHAT YOU ARE DOING,"$N
echo -e $R"THEN PLEASE DON'T PROCEED. I WON'T BE RESPONSIBLE"$N
echo -e $R"FOR THE MESS YOU CREATE."$N
echo -e "$divider"
sleep 5

# Check Energized Directory
clear
echo "[+] Checking Energized Directory."; sleep 0.1; clear
echo "[+] Checking Energized Directory.."; sleep 0.2; clear
echo "[+] Checking Energized Directory..."; sleep 0.1; clear

# Check if the Whitelist is there, else creates
if [ ! -f /sdcard/EnergizedProtection/blacklist ]; then
   clear
   echo "\n[+] Creating Blacklist.\n";
   touch /sdcard/EnergizedProtection/blacklist
   sleep 0.2
fi

# Check if the Blacklist is there, else creates 
if [ ! -f /sdcard/EnergizedProtection/whitelist ]; then
   clear
   echo "[+] Creating Whitelist.";
   touch /sdcard/EnergizedProtection/whitelist
   sleep 0.2
fi

# Check if the temp is there, else creates
if [ ! -f /sdcard/EnergizedProtection/cache/energizedtemp ]; then
   clear
   echo "\n[+] Creating temporary file [1/3].\n";
   touch /sdcard/EnergizedProtection/cache/energizedtemp
   sleep 0.2
fi

# Check if the porn temp is there, else creates
if [ ! -f /sdcard/EnergizedProtection/cache/energizedporntemp ]; then
   clear
   echo "\n[+] Creating temporary file [2/3].\n";
   touch /sdcard/EnergizedProtection/cache/energizedporntemp
   sleep 0.2
fi

# Check if the social temp is there, else creates
if [ ! -f /sdcard/EnergizedProtection/cache/energizedsocialtemp ]; then
   clear
   echo "\n[+] Creating temporary file [3/3].\n";
   touch /sdcard/EnergizedProtection/cache/energizedsocialtemp
   sleep 0.2
fi

