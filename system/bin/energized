while true
do

# Aliases, Grep, Wget Variables
HOST=/system/etc/hosts
TREADME=/sdcard/EnergizedProtection/readme
FILTER=/sdcard/EnergizedProtection/filter
WHITELIST=/sdcard/EnergizedProtection/whitelist
BLACKLIST=/sdcard/EnergizedProtection/blacklist
TEMP=/sdcard/EnergizedProtection/cache/energizedtemp
PTEMP=/sdcard/EnergizedProtection/cache/energizedporntemp
STEMP=/sdcard/EnergizedProtection/cache/energizedsocialtemp
COUNT=1

# Check if Root was granted or not
id="$(id)"; id="${id#*=}"; id="${id%%\(*}"; id="${id%% *}"
if [ "$id" != "0" ] && [ "$id" != "root" ]; then
    root="\e[1;31m[×]\e[0m";
else
    root="\e[1;32m[✓]\e[0m";
fi;

# Other Variables
update="\e[1;36m`busybox date -r /system/etc/hosts`\e[0m"
bboxname="\e[1;36m`busybox | head -1 | cut -f 2 -d ' '`\e[0m"
size="\e[1;35m`ls -lah /system/etc/hosts | awk '{print $5}'`\e[0m"

# Check if System's Adblocker is enabled or not
   [ -f /system/etc/hosts ];
if busybox grep -q adblocker /system/etc/hosts; then
	adblocker="\e[1;32m[✓]\e[0m"
else
	adblocker="\e[1;31m[×]\e[0m"
fi

# Check if Energized is applied or not
	[ -f /system/etc/hosts ];
if busybox grep -q nayemador.com /system/etc/hosts; then
	eonoff="\e[1;32m[✓]\e[0m"
else
	eonoff="\e[1;31m[×]\e[0m"
fi

# If Energized is enabled, which pack it is
	[ -f /system/etc/hosts ];
if busybox grep -q "E84S1C-P" /system/etc/hosts; then
	echeck="\e[1;32mBasic Protection\e[0m"
elif busybox grep -q "E5P4RK-P" /system/etc/hosts; then
	echeck="\e[1;32mSpark Protection\e[0m"	
elif busybox grep -q "E8LU-P" /system/etc/hosts; then
	echeck="\e[1;32mBlu Protection\e[0m"
elif busybox grep -q "E8LUG0-P" /system/etc/hosts; then
	echeck="\e[1;32mBlu Go Protection\e[0m"
elif busybox grep -q "EP0R9-P" /system/etc/hosts; then
	echeck="\e[1;32mPorn Protection\e[0m"
elif busybox grep -q "EUL71M473-P" /system/etc/hosts; then
	echeck="\e[1;32mUltimate Protection\e[0m"
elif busybox grep -q "EU91F13D-P" /system/etc/hosts; then
	echeck="\e[1;32mUnified Protection\e[0m"
else
	echeck="\e[1;31mNo Pack Detected!\e[0m"
